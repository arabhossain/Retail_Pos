<AnchorPane id="AnchorPane" prefHeight="426.0" prefWidth="975.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ChangePasswordController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Change Password" textFill="#0beb21">
         <font>
            <Font name="Constantia Bold" size="18.0" />
         </font>
      </Label>
      <Separator layoutX="14.0" layoutY="45.0" prefHeight="0.0" prefWidth="779.0" />
      <Label layoutX="21.0" layoutY="129.0" text="Old Password" />
      <PasswordField fx:id="Old" layoutX="124.0" layoutY="125.0" prefHeight="25.0" prefWidth="248.0" />
      <Label layoutX="21.0" layoutY="202.0" text="New Password" />
      <Label layoutX="21.0" layoutY="238.0" text="Re-Password" />
      <PasswordField fx:id="New" layoutX="124.0" onKeyReleased="#MatchRe"  layoutY="198.0" prefHeight="25.0" prefWidth="248.0" />
      <PasswordField fx:id="Re" layoutX="124.0" onKeyReleased="#MatchRe"  layoutY="234.0" prefHeight="25.0" prefWidth="248.0" />
      <ImageView fx:id="imgRe" fitHeight="32.0" fitWidth="32.0" layoutX="391.0" layoutY="210.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="/images/pay.png" />
         </image>
      </ImageView>
      <Button fx:id="btnChangePass" defaultButton="true" layoutX="222.0" layoutY="278.0" mnemonicParsing="false" onAction="#btnChangeEvent" prefHeight="55.0" prefWidth="150.0" text="Change Password" />
   </children>
</AnchorPane>
