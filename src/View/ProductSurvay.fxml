         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="253.0" layoutY="200.0" text="Visible" />
      <CheckBox fx:id="checkStatus" layoutX="309.0" layoutY="200.0" mnemonicParsing="false">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></CheckBox>
      <Button defaultButton="true" layoutX="298.0" layoutY="267.0" mnemonicParsing="false" onAction="#btnSave" prefHeight="54.0" prefWidth="139.0" text="Save ">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <ComboBox fx:id="comboShortByAI" layoutX="355.0" layoutY="66.0" onMouseClicked="#shortByAI" prefWidth="150.0" promptText="Sort by" />
   </children>
</AnchorPane>
